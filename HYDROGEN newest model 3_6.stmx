			</aux>
			<aux name="Electricity Demand Growth per Year">
				<doc>https://www.eia.gov/todayinenergy/detail.php?id=49036</doc>
				<eqn>1.01</eqn>
				<units>Dimensionless</units>
			</aux>
			<stock name="Overall Electricity\nDemand">
				<doc>(8.6e6)*4.6633

https://www.sciencedirect.com/science/article/pii/S2215017X20300813</doc>
				<eqn>{40104380}108e6</eqn>
				<inflow>New_Growth_in_Electricity_Demand</inflow>
				<units>MWh/Year</units>
			</stock>
			<flow name="New Growth in Electricity Demand">
				<eqn>((Population*&quot;Electricity_Demand_per_Capita(2025)&quot;)-Overall_Electricity_Demand)/Adjustment_Time_for_Overall_Electricity_Demand</eqn>
				<units>MWh/Year/Years</units>
			</flow>
			<stock name="Growth in\nElectricity Demand">
				<eqn>1</eqn>
				<inflow>New_Growth_in_Electricity_Demand_Fraction</inflow>
				<outflow>Demand_Returning_to_Normal</outflow>
				<units>Dimensionless</units>
			</stock>
			<flow name="New Growth in Electricity Demand Fraction">
				<eqn>(Initial_Growth_Factor/Adjustment_Time_For_Growth)*(1+STEP(Scenario_for_Growth_in_Demand, Time_for_Growth_to_Start)-STEP(Scenario_for_Growth_in_Demand, Time_for_Growth_to_Stop))</eqn>
				<non_negative/>
				<units>Dimensionless/Year</units>
			</flow>
			<flow name="Demand Returning\nto Normal">
				<eqn>Growth_in_Electricity_Demand/Time_to_Return_to_Normal</eqn>
				<non_negative/>
				<units>Dimensionless/Year</units>
			</flow>
			<aux name="Initial Growth\nFactor">
				<eqn>1</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Scenario for Growth\nin Demand">
				<eqn>.01</eqn>
				<format precision="0.01" scale_by="1"/>
				<range min="0" max="0.5"/>
				<units>Dimensionless</units>
			</aux>
			<aux name="Time for Growth\nto Start">
				<eqn>2030</eqn>
				<format precision="1" scale_by="1"/>
				<range min="2025" max="2050"/>
				<units>Years</units>
			</aux>
			<aux name="Time for Growth\nto Stop">
				<eqn>2050</eqn>
				<format precision="1" scale_by="1"/>
				<range min="2025" max="2051"/>
				<units>Years</units>
			</aux>
			<aux name="Adjustment Time\nFor Growth">
				<eqn>5</eqn>
				<units>Years</units>
			</aux>
			<aux name="Time to Return\nto Normal">
				<eqn>5</eqn>
				<units>Years</units>
			</aux>
			<aux name="Demand for\nElectricity">
				<eqn>Overall_Electricity_Demand*Growth_in_Electricity_Demand</eqn>
				<units>MWh/Year</units>
			</aux>
			<aux name="Adjustment Time\nfor Overall Electricity Demand">
				<eqn>3</eqn>
