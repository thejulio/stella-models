			<aux name="Percent of income from electricity sales reinvested in plants">
				<doc>https://einvestingforbeginners.com/rd-spending-as-a-percentage-of-revenue-by-industry/</doc>
				<eqn>1-&quot;Percent_of_Income_from_Electricity_Sales_Reinvested_in_R&amp;D&quot;</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Average Annual Hydrogen Per Year at Capacity">
				<eqn>Average_Annual_Hydrogen_Per_Year*Capacity_Factor_for_Electrolysis_Plants</eqn>
				<units>Tons/Year</units>
			</aux>
			<aux name="Capacity Factor for Electrolysis Plants">
				<eqn>0.9</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Hydrogen Fuel Cell Capacity Factor">
				<doc>https://www.hydrogen.energy.gov/docs/hydrogenprogramlibraries/pdfs/review16/tv016_saur_2016_p.pdf</doc>
				<eqn>.892</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Percent of Income from Electricity Sales Reinvested in R&amp;D">
				<doc>https://einvestingforbeginners.com/rd-spending-as-a-percentage-of-revenue-by-industry/</doc>
				<eqn>0.006</eqn>
				<units>Dimensionless</units>
			</aux>
			<stock name="Total Profit">
				<eqn>0</eqn>
				<inflow>Additions_to_Profit</inflow>
				<units>Dollars</units>
			</stock>
			<flow name="Additions to Profit">
				<eqn>Income_from_Electricity_Sales-Added_Cost</eqn>
				<units>Dollars/Years</units>
			</flow>
			<aux name="Money for\nNew Plants">
				<eqn>Cost_Per_Plant*Plants_Retired</eqn>
				<units>Dollars/Years</units>
			</aux>
			<stock name="Electrolyzers">
				<eqn>0</eqn>
				<inflow>Revamped_Electrolyzers</inflow>
				<outflow>Electrolyzers_Removed</outflow>
				<units>Electrolyzers</units>
			</stock>
			<flow name="Revamped\nElectrolyzers">
				<eqn>(Needed_Electrolyzers/Adjustment_Time_for_Electrolyzers)</eqn>
				<non_negative/>
				<units>Electrolyzers/Years</units>
			</flow>
			<flow name="Electrolyzers\nRemoved">
				<eqn>Electrolyzers/Average_Life_of_Electrolyzers</eqn>
				<non_negative/>
				<units>Electrolyzers/Years</units>
			</flow>
			<aux name="Average Life\nof Electrolyzers">
				<eqn>6</eqn>
				<units>Years</units>
			</aux>
			<aux name="Electrolysers Per Plant">
				<doc>https://austinpowereng.com/Fuel%20cell/Yang_AustinPower%20H2Electrolysis%20FCS2019.pdf</doc>
				<eqn>4</eqn>
				<units>Electrolyzers/Plant</units>
			</aux>
			<aux name="Desired Number\nof Electrolyzers">
				<eqn>Electrolysers_Per_Plant*&quot;Electrolysis_Plants/Facilities&quot;</eqn>
				<units>Electrolyzers</units>
			</aux>
			<aux name="Needed\nElectrolyzers">
				<eqn>Desired_Number_of_Electrolyzers-Electrolyzers</eqn>
				<units>Electrolyzers</units>
			</aux>
			<aux name="Adjustment Time for Electrolyzers">
				<eqn>1</eqn>
				<units>Years</units>
			</aux>
			<aux name="Cost to Revamp\nElectrolyzers">
				<eqn>2000000</eqn>
				<units>Dollars/MWh</units>
			</aux>
			<aux name="Average Cost to\nRevamp Plant">
				<eqn>Cost_Per_Plant*Fraction_of_Plant_Cost_for_Electrolyzers</eqn>
				<units>Dollars/Plant</units>
			</aux>
			<aux name="Plants Revamped">
				<eqn>Electrolyzers_Removed/Electrolysers_Per_Plant</eqn>
				<units>Plant/Years</units>
			</aux>
			<aux name="Yearly Cost to\nRevamp Plants">
				<eqn>Average_Cost_to_Revamp_Plant*Plants_Revamped</eqn>
				<units>Dollars/Years</units>
			</aux>
			<aux name="Fraction of Plant Cost for Electrolyzers">
				<doc>https://www.energypolicy.columbia.edu/demystifying-electrolyzer-production-costs/</doc>
				<eqn>0.3</eqn>
				<units>Dimensionless</units>
			</aux>
			<stock name="Annual\nProfit">
				<eqn>0</eqn>
				<inflow>Net_Change_in_Average_Annual_Profit</inflow>
				<units>Dollars/Years</units>
			</stock>
			<flow name="Net Change in Average Annual Profit">
				<eqn>(Additions_to_Profit-Annual_Profit)/Time_to_Adjust_Yearly_Spending</eqn>
				<units>Dollars/Years/Years</units>
			</flow>
			<aux name="Average Electrolysers\nPer Plant">
				<eqn>Electrolyzers//&quot;Electrolysis_Plants/Facilities&quot;</eqn>
				<units>Electrolyzers/Plant</units>
			</aux>
