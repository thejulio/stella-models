				<gf>
					<xscale min="0" max="1.03"/>
					<yscale min="0" max="1"/>
					<ypts>1,0.9954,0.9879,0.9755,0.9552,0.922,0.8676,0.7784,0.6322,0.3926,0</ypts>
				</gf>
				<units>Dimensionless</units>
			</aux>
			<aux name="Electrolysis Plant Efficiency">
				<doc>https://search.lib.jmu.edu/permalink/01JMU_INST/1jlet4m/alma991004417429706271</doc>
				<eqn>.625</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="MWh per Ton of Hydrogen ( E efficiency)">
				<eqn>MWh_per_Ton_of_Hydrogen*Electrolysis_Plant_Efficiency</eqn>
				<units>MWh/Tons</units>
			</aux>
			<aux name="Hydrogen Fuel\nCell Efficiency">
				<doc>https://www.sciencedirect.com/book/9780444536884/pem-fuel-cell-testing-and-diagnosis?via=ihub=</doc>
				<eqn>.353</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Actual MWh per Ton of Hydrogen">
				<eqn>&quot;MWh_per_Ton_of_Hydrogen_(_E_efficiency)&quot;*Hydrogen_Fuel_Cell_Efficiency*Hydrogen_Fuel_Cell_Capacity_Factor</eqn>
				<units>MWh/Tons</units>
			</aux>
			<aux name="Political Will">
				<eqn>Ratio_of_Supply_to_Demand</eqn>
				<units>Dimensionless</units>
			</aux>
			<stock name="Cumulative Cost">
				<eqn>0</eqn>
				<inflow>Added_Cost</inflow>
				<units>Dollars</units>
			</stock>
			<flow name="Added Cost">
				<eqn>Electrolysis_Plant_Costs+Fuel_Cell_Costs+&quot;R&amp;D_Investment&quot;+&quot;R&amp;D_Investment(FC)&quot;+Yearly_Cost_to_Revamp_Plants+Price_of_electricity_for_producing_carbon</eqn>
				<non_negative/>
				<units>Dollars/Years</units>
			</flow>
			<aux name="Electrolysis Plant Costs">
				<eqn>Additions_to_Electrolysis_Plants*Cost_Per_Plant</eqn>
				<units>Dollar/Years</units>
			</aux>
			<aux name="Fuel Cell Costs">
				<eqn>&quot;Cost_Per_Plant(FC)&quot;*Additions_to_Fuel_Cell_Plants</eqn>
				<units>Dollar/Years</units>
			</aux>
			<stock name="Average Annual\nCost">
				<eqn>0</eqn>
				<inflow>Net_Change_in_Average_Annual_Cost</inflow>
				<units>Dollars/Years</units>
			</stock>
			<flow name="Net Change in Average Annual Cost">
				<eqn>(Added_Cost-Average_Annual_Cost)/Time_to_Adjust_Yearly_Spending</eqn>
				<units>Dollars/Years/Years</units>
			</flow>
			<aux name="Time to Adjust\nYearly Spending">
				<eqn>3</eqn>
				<units>Years</units>
			</aux>
			<aux name="Initial Investment\nin Plants">
				<eqn>750e6</eqn>
				<units>Dollars</units>
			</aux>
			<aux name="Price per MWh">
				<eqn>150</eqn>
				<units>Dollars/MWh</units>
			</aux>
			<aux name="Income from Electricity Sales">
				<eqn>Average_Annual_MWh_from_Hydrogen*Price_per_MWh</eqn>
				<units>Dollars/Years</units>
			</aux>
			<aux name="Fraction of Electricity\nIncome Reinvested">
				<eqn>.1</eqn>
				<units>Dimensionless</units>
			</aux>
			<aux name="Income from Electricity Sales Reinvested">
				<eqn>Fraction_of_Electricity_Income_Reinvested*Income_from_Electricity_Sales</eqn>
				<units>Dollars/Years</units>
			</aux>
			<stock name="Hydrogen From\nPlants">
				<eqn>0</eqn>
				<inflow>Additions_to_Hydrogen_per_Plant</inflow>
				<outflow>Hydrogen_per_Plant_Removed</outflow>
				<units>Tons/Year</units>
			</stock>
			<flow name="Additions to Hydrogen\nper Plant">
				<eqn>Additions_to_Electrolysis_Plants*Tons_per_Plant_Per_year*&quot;Implementation_Delay_(F/y)&quot;</eqn>
				<non_negative/>
				<units>Tons/Year/Years</units>
			</flow>
			<flow name="Hydrogen per\nPlant Removed">
				<eqn>Average_Hydrogen_per_Plant*Plants_Retired</eqn>
				<non_negative/>
				<units>Tons/Year/Years</units>
			</flow>
			<aux name="Average Hydrogen per Plant">
				<eqn>(Hydrogen_From_Plants)//(&quot;Electrolysis_Plants/Facilities&quot;)</eqn>
				<units>Tons/Year/Plant</units>
			</aux>
